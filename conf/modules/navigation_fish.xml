<!DOCTYPE module SYSTEM "module.dtd">
<module name="navigation_fish" dir="nav">
	<doc>
		<description>Navigate Bebops like fish</description>
	</doc>

<settings target="ap|nps">
  <dl_settings>

    <dl_settings NAME="Navigation Fish Parameters">
      <dl_setting var="ang" min="0.1" step="1.0" max="360.0" module="modules/nav/navigation_fish" shortname="angle" handler="setAngleParameter"/>
 	<dl_setting var="YW" min="0.01" step=".01" max="1.0" module="modules/nav/navigation_fish"/>
	<dl_setting var="DIR_FLUCT" min="0.01" step=".01" max="1.0" module="modules/nav/navigation_fish"/>
	<dl_setting var="YATT" min="0.01" step=".01" max="1.0" module="modules/nav/navigation_fish"/>
	<dl_setting var="LATT" min="0.01" step=".1" max="5.0" module="modules/nav/navigation_fish"/>
	<dl_setting var="D0ATT" min="0.01" step=".1" max="5.0" module="modules/nav/navigation_fish"/>
	<dl_setting var="YALI" min="0.01" step=".01" max="1.0" module="modules/nav/navigation_fish"/>
	<dl_setting var="LALI" min="0.01" step=".1" max="5.0" module="modules/nav/navigation_fish"/>
	<dl_setting var="D0ALI" min="0.01" step=".1" max="5.0" module="modules/nav/navigation_fish"/>
   	<dl_setting var="myneighbor" min="200" step="1" max="203" module="modules/nav/navigation_fish"/>
    </dl_settings>






  </dl_settings>
</settings>



	<header>
    		<file name="navigation_fish.h"/>
	</header>
	<periodic fun="nav_fish_run_velocity()" freq="1." autorun="FALSE" /> 
	
	<periodic fun="nav_fish_run_position()" freq="16." autorun="FALSE" />
	<periodic fun="axis_test()" freq="1." autorun="FALSE" />
	<periodic fun="comm_test()" freq="1." autorun="FALSE" />
	<periodic fun="speed_test()" freq="1." autorun="FALSE" />
	<periodic fun="parameter_data()" period="65" autorun="FALSE" />
	
 	<makefile>
    		<file name="navigation_fish.c"/>
  	</makefile>
</module>
